<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="330.0" prefWidth="672.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.wenjiachen.bank.controller.view.MainViewController">
   <children>
      <Label fx:id="UserTitle" layoutX="81.0" layoutY="61.0" prefHeight="35.0" prefWidth="481.0" text="欢迎您，{UserName}">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="BalanceButton" layoutX="81.0" layoutY="140.0" mnemonicParsing="false" onAction="#onBalanceButtonClicked" styleClass="btn-primary" text="账户余额查询">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="HistoryButton" layoutX="82.0" layoutY="211.0" mnemonicParsing="false" styleClass="bg-primary" text="历史交易查询">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="TransButton" layoutX="285.0" layoutY="140.0" mnemonicParsing="false" styleClass="bg-primary" text="账户转账">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="UserBaseInfoButton" layoutX="285.0" layoutY="211.0" mnemonicParsing="false" styleClass="bg-primary" text="基本信息">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="AccountButton" layoutX="462.0" layoutY="140.0" mnemonicParsing="false" styleClass="bg-primary" text="存款取款">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="PwdChangedButton" layoutX="462.0" layoutY="211.0" mnemonicParsing="false" styleClass="bg-primary" text="修改密码">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
